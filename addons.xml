    </extension>
</addon>

<addon
    id="script.pseudo.library"
    version="0.0.8"
    name="PseudoLibrary"
    provider-name="Lunatixz">
    <requires>
        <import addon="script.common.plugin.cache" version="0.9.1"/>
        <import addon="script.module.requests" version="1.1.0"/>
        <import addon="script.module.simplejson" version="0.9.1"/>
